import java.util.*;

public class Main 
{
    public static void main(String[] args)
    {
      int arr[]={21,2,30,10,5,19};
      int n=10;
      int searchedNumber=searchTheNumberInAnArray(n,arr);
      System.out.println(searchedNumber);
    }
    
    public static int searchTheNumberInAnArray(int n,int[] arr)
    {
      
      for (int i=0;i<arr.length;i++)
      {
        if(n==arr[i])
        return n;
      }
      return 0;
    }
    
}


//output:10
