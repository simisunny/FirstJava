import java.util.*;

public class Main 
{
  public static void main(String[] args) 
    {
      fibonacciSeries();//calling the function
    }
  public static void fibonacciSeries() 
  {
    long i=0;
    long j=1;
    System.out.print(i+" "+j+" ");// print first and second element
    
    for (int loop=2;loop<20 ;loop++) //loop for the number of elements we want to show
    {
       long k=0;
       k=j+i;//adding previous two numbers to get the third number
       System.out.print(k+" ");//print k
       
       i=j;//storing the second element into the first
       j=k;//storing the third element into the second
    }
  }
}
  
