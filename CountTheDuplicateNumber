import java.util.*;

public class Main 
{
    public static void main(String[] args) 
    {
      
      int[] arr={3,7,18,1,5,5,9};//find the duplicate number given that there is only one duplicate number in an array
      int n=5;
      int number=CountTheDuplicateNumber(arr,n);
      System.out.println("The duplicate number is found "+number+" times");
    }
    public static int CountTheDuplicateNumber(int[] arr,int n)
    {
     int count=0;
      for(int i=0;i<arr.length;i++)
{
if(arr[i]==n)
{

count++;

}

    }
   return count; 
}
}
