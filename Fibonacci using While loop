import java.util.*;

public class Main {
    public static void main(String[] args) 
    {
      int i=0;int j=1;int k=0;int loop=2;
      
      System.out.print(i+" ");
      System.out.print(j+" ");
      
      while(loop<4)
      {
        k=i+j;
        loop++;
        System.out.print(k+" ");
        i=j;
        j=k;
      }
    }
}
