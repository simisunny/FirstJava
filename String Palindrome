import java.util.*;

class Test 
{
    public static void main(String args[]) 
    {
       String str="simi"; String str1=""; boolean flag=true;
       for(int i=str.length()-1;i>=0;i--)
{
           str1=str1+str.charAt(i);
       

           if(str.equals(str1))
       {
          flag=false;
           System.out.println("Palindrome");
         
       }
       
}
    
    if(flag==true)
    {
      System.out.println("Not Palindrome");
    }
   }
}
